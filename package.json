{
  "name": "pinterest-clone",
  "version": "1.0.0",
  "description": "A simple pinterest clone",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.js",
    "build:client": "cross-env NODE_ENV=production webpack",
    "build": "yarn run clean && yarn run build:server && yarn run build:client",
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "heroku-postbuild": "yarn run build",
    "lint": "tslint \"src/**/*.ts\"",
    "lint:fix": "yarn run lint -- --fix",
    "start": "cross-env NODE_ENV=production node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksmai/pinterest-clone.git"
  },
  "keywords": [
    "pinterest",
    "mean"
  ],
  "author": "ksmai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ksmai/pinterest-clone/issues"
  },
